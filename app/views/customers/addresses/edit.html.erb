<h1>配送先編集</h1>
<div>
  <%= form_with model: @address, local:true do |f| %>
  <table>
    <tr>
      <th></th>
      <th></th>
    </tr>
    <tr>
      <td>郵便番号（ハイフンなし）</td>
      <td><%= f.text_field :postal_code %></td>
    </tr>
    <tr>
      <td>住所</td>
      <td><%= f.text_field :address %></td>
    </tr>
    <tr>
      <td>宛名</td>
      <td><%= f.text_field :name %></td>
    </tr>
    <%= f.submit '変更内容を保存' %>
  </table>
  <% end %>
</div>